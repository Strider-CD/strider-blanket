{
  "name": "strider-blanket",
  "version": "0.0.1",
  "description": "Blanket.js for your project",
  "main": "index.js",
  "scripts": {
    "test": "make test"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/Strider-CD/strider-blanket"
  },
  "keywords": [
    "strider",
    "plugin",
    "coverage",
    "blanket",
    "blanket.js"
  ],
  "author": "Jared Forsyth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Strider-CD/strider-blanket/issues"
  },
  "dependencies": {
    "mocha": "~1.14.0",
    "jade": "~0.35.0"
  },
  "strider": {
    "id": "blanket",
    "type": "job",
    "title": "Blanket",
    "webapp": "webapp.js",
    "worker": "worker.js",
    "icon": "icon.png",
    "config": true,
    "build-status": {
      "style": "config/build.less",
      "template": "config/build.html",
      "attrs": {
        "ng-show": "data.enabled",
        "class": "[[ data.coverageStatus ]]"
      }
    }
  }
}
